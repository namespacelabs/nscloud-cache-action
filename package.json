{
  "name": "@namespacelabs/nscloud-cache-action",
  "version": "1.0.0",
  "private": true,
  "description": "Cache artifacts like dependencies and build outputs to a Namespace Cloud cache volume to improve workflow execution time.",
  "homepage": "https://namespace.so",
  "main": "dist/index.js",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "ncc build -o dist/index src/index.ts && ncc build -o dist/post src/post.ts",
    "format": "prettier --no-error-on-unmatched-pattern --config ./.prettierrc.js --write \"**/*.{ts,yml,yaml}\"",
    "format:check": "prettier --no-error-on-unmatched-pattern --config ./.prettierrc.js --check \"**/*.{ts,yml,yaml}\"",
    "lint": "eslint --config ./.eslintrc.js \"**/*.ts\"",
    "lint:fix": "eslint --config ./.eslintrc.js \"**/*.ts\" --fix",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "pre-checkin": "npm run format && npm run lint:fix && npm run build && npm test run"
  },
  "author": "Namespace Labs",
  "dependencies": {
    "@actions/core": "^2.0.2",
    "@actions/exec": "^2.0.0",
    "@actions/github": "^7.0.0",
    "@actions/io": "^2.0.0",
    "@actions/tool-cache": "^3.0.0",
    "json-multi-parse": "^1.3.1",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.3"
  }
}
